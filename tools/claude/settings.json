{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/stop-notification.sh"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/permission-notification.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "firebase@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "context7@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "figma@claude-plugins-official": true,
    "Notion@claude-plugins-official": true,
    "github@claude-plugins-official": true,
    "vercel@claude-plugins-official": true,
    "slack@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "nextjs-vercel-pro@claude-code-templates": true,
    "devops-automation@claude-code-templates": true,
    "testing-suite@claude-code-templates": true,
    "git-workflow@claude-code-templates": true
  },
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754066768978
  }
}
